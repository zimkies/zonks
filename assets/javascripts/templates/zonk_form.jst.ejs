<h1>New Award</h1>
<form role='form' class='form-horizontal'>
  <fieldset>
    <div class='form-group'>
      <input class="form-control" placeholder='Teammate name'></input>
    </div>
    <div class='form-group'>
      <select class="form-control" type='select' placeholder='Zonk'>
        <% zonks.forEach(function(el) { %>
          <option value="<%= el.name %>"><%= el.name %></option>
        <% }); %>
      </select>
    </div>
    <div class='form-group'>
      <textarea class="form-control" placeholder='Deed'></textarea>
    </div>
  </fieldset>

  <div class='form-group'>
    <input class="btn btn-default" type="submit"></input>
  </div>
</form>
