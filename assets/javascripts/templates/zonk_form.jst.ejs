<h3>New Award</h3>
<form role='form' class='form-inline'>
  <div class='form-group'>
    <input class="form-control" placeholder='Teammate name(s)' name="name"></input>
  </div>
  <div class='form-group'>
    <select name="zonk" class="form-control" type='select' placeholder='Zonk'>
      <% zonks.forEach(function(el) { %>
        <option value="<%= el.name %>"><%= el.name %></option>
      <% }); %>
    </select>
  </div>
  <div class='form-group'>
    <textarea class="form-control" name="deed" placeholder='and earned it by...'></textarea>
  </div>

  <div class='form-group'>
    <input class="btn btn-primary" type="submit"></input>
  </div>
</form>
